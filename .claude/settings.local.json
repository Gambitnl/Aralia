{
  "permissions": {
    "allow": [
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(git:*)",
      "Bash(node:*)",
      "Bash(python:*)",
      "Bash(powershell:*)",
      "Bash(cargo:*)",
      "Bash(codexbar-cli:*)",
      "Bash(cat:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(rm:*)",
      "Bash(mv:*)",
      "Bash(dir:*)",
      "Bash(mkdir:*)",
      "Bash(echo:*)",
      "Bash(curl:*)",
      "Bash(timeout:*)",
      "Bash(xargs:*)",
      "Bash(rev:*)",
      "Bash(findstr:*)",
      "Bash(tasklist:*)",
      "Bash(where:*)",
      "Bash(set:*)",
      "Bash(for:*)",
      "Bash(gh:*)",
      "Bash(pnpm:*)",
      "Bash(ping:*)",
      "Bash(netstat:*)",
      "Bash(taskkill:*)",
      "mcp__github__get_file_contents",
      "Skill(claudeception)",
      "Skill(claudeception:*)",
      "Bash(python3 -c \" import sys, json for line in sys.stdin: try: obj = json.loads\\(line.strip\\(\\)\\) ts = obj.get\\(''timestamp'', obj.get\\(''created'', ''''\\)\\) if ''2026-02-05'' in str\\(ts\\) or ''2026-02-06T0'' in str\\(ts\\): print\\(json.dumps\\({k: obj[k] for k in [''sessionId'',''timestamp'',''summary'',''projectPath''] if k in obj}, indent=2\\)\\) except: pass \")",
      "Bash(SRC=\"C:/Users/Gambit/.claude/projects/R--AraliaV4-Aralia\")",
      "Bash(DST=\"C:/Users/Gambit/.claude/projects/f--Repos-Aralia\")",
      "Bash(do cp -v \"$f\" \"$DST/\")",
      "Bash(done)",
      "Bash(chmod:*)"
    ],
    "deny": [],
    "ask": []
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "chrome-devtools",
    "github",
    "image-gen"
  ],
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "node .claude/hooks/file-backup-hook.js",
            "timeout": 10
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "node .claude/hooks/session-cleanup-hook.js"
          }
        ]
      }
    ]
  }
}
