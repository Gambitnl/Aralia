{
  "mcpServers": {
    "chrome-devtools": {
      "command": "node",
      "args": [
        "scripts/run-chrome-mcp.js",
        "--browserUrl",
        "http://127.0.0.1:9222"
      ]
    },
    "github": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    },
    "image-gen": {
      "command": "npx.cmd",
      "args": [
        "tsx",
        "scripts/image-gen-mcp.ts"
      ]
    },
    "stitch": {
      "command": "npx.cmd",
      "args": [
        "@_davideast/stitch-mcp",
        "proxy",
        "--debug"
      ],
      "env": {
        "STITCH_PROJECT_ID": "your-project-id"
      }
    }
  }
}
