{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx.cmd",
      "args": [
        "chrome-devtools-mcp@latest",
        "--browserUrl",
        "http://127.0.0.1:9222"
      ]
    },
    "my-image-gen": {
      "command": "npx.cmd",
      "args": [
        "tsx",
        "C:/Users/gambi/Documents/Git/AraliaV4/Aralia/scripts/gemini-image-mcp.ts"
      ]
    },
    "github": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN_HERE>"
      }
    }
  }
}