{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Race QABatch Output",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "batchId",
    "entries",
    "raceProfiles"
  ],
  "properties": {
    "batchId": {
      "type": "string",
      "minLength": 1
    },
    "entries": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "raceId",
          "gender",
          "visualStatus",
          "uniquenessStatus",
          "manualReviewRequired",
          "checklist",
          "likelyScore",
          "likelyReason",
          "targetActivity",
          "notes"
        ],
        "properties": {
          "raceId": {
            "type": "string",
            "minLength": 1
          },
          "gender": {
            "type": "string",
            "enum": [
              "male",
              "female"
            ]
          },
          "visualStatus": {
            "type": "string",
            "enum": [
              "pending",
              "approved",
              "rejected"
            ]
          },
          "uniquenessStatus": {
            "type": "string",
            "enum": [
              "pending",
              "unique",
              "keeper",
              "non_keeper"
            ]
          },
          "manualReviewRequired": {
            "type": "boolean"
          },
          "checklist": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "isSquare",
              "isFullBody",
              "isEdgeToEdge",
              "isSliceOfLife",
              "isCivilian",
              "hasArrowsArtifact"
            ],
            "properties": {
              "isSquare": {
                "type": [
                  "boolean",
                  "null"
                ]
              },
              "isFullBody": {
                "type": [
                  "boolean",
                  "null"
                ]
              },
              "isEdgeToEdge": {
                "type": [
                  "boolean",
                  "null"
                ]
              },
              "isSliceOfLife": {
                "type": [
                  "boolean",
                  "null"
                ]
              },
              "isCivilian": {
                "type": [
                  "boolean",
                  "null"
                ]
              },
              "hasArrowsArtifact": {
                "type": [
                  "boolean",
                  "null"
                ]
              }
            }
          },
          "likelyScore": {
            "type": [
              "integer",
              "null"
            ],
            "minimum": 1,
            "maximum": 5
          },
          "likelyReason": {
            "type": [
              "string",
              "null"
            ]
          },
          "targetActivity": {
            "type": [
              "string",
              "null"
            ]
          },
          "notes": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      }
    },
    "raceProfiles": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "raceId",
          "raceName",
          "summary",
          "answers"
        ],
        "properties": {
          "raceId": {
            "type": "string",
            "minLength": 1
          },
          "raceName": {
            "type": "string",
            "minLength": 1
          },
          "summary": {
            "type": "string",
            "minLength": 1
          },
          "answers": {
            "type": "array",
            "minItems": 10,
            "maxItems": 10,
            "items": {
              "type": "object",
              "additionalProperties": false,
              "required": [
                "questionId",
                "question",
                "answer"
              ],
              "properties": {
                "questionId": {
                  "type": "string",
                  "pattern": "^q([1-9]|10)$"
                },
                "question": {
                  "type": "string",
                  "minLength": 1
                },
                "answer": {
                  "type": "string",
                  "minLength": 1
                }
              }
            }
          }
        }
      }
    }
  }
}
